# shift modifier doesn't seem to work (at least not on NCD HMX)
#Button1 = shift : root : f.menu "xterm"
#Button2 = shift : root : f.menu "wmops"
#Button3 = shift : root : f.menu "miscapps"

Button1 = control : root : f.menu "slogin"
Button2 = control : root : f.menu "tools"
Button3 = control : root : f.menu "games"

Button1 = meta : window|icon : f.function "forcemove-or-raise"
Button2 = meta : window|icon : f.function "forcemove-or-iconify"
